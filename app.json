{
  "expo": {
    "name": "SelfQare",
    "slug": "selfqare-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/selfqare-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/selfqare-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1443C3"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/selfqare-adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "experiments": {
      "tsconfigPaths": true
    }
  }
}
